<p id="notice"><%= notice %></p>

<h1>Meal Logs</h1>

<table>
  <thead>
    <tr>
      <th>Pet</th>
      <th>Food id</th>
      <th>Food amount</th>
      <th>Photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meal_logs.each do |meal_log| %>
      <tr>
        <td><%= meal_log.pet_id %></td>
        <td><%= meal_log.food_id %></td>
        <td><%= meal_log.food_amount %></td>
        <td><%= meal_log.photo_id %></td>
        <td><%= link_to 'Show', meal_log %></td>
        <td><%= link_to 'Edit', edit_meal_log_path(meal_log) %></td>
        <td><%= link_to 'Destroy', meal_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meal Log', new_meal_log_path %>
